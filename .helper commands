/* app-material-module */

// MaterialDatePicker
ng g c app-material-demo/components/MaterialDatePicker --module=app-material-demo/app-material-demo.module.ts

// MaterialFormField
ng g c app-material-demo/components/MaterialFormField --module=app-material-demo/app-material-demo.module.ts


// Routing module
ng generate module app-material-demo/app-material-demo-routing --flat --module=app-material-demo/app-material-demo.module.ts

/* app-module */

// Home Component
ng g c components/Home

// About Component
ng g c components/About

// Page Not Found
ng g c components/PageNotFound

/* app-bootstrap-module */

// Create module
ng g module app-bootstrap

// Create Bootstrap Routing module
ng generate module app-bootstrap/app-bootstrap-routing --flat --module=app-bootstrap/app-bootstrap.module.ts

// Create Accordion Component
ng g c app-bootstrap/components/bootstrap-accordion/accordion/BootstrapAccordion --module=app-bootstrap/app-bootstrap.module.ts 

// Create Alert Component
ng g c app-bootstrap/components/BootstrapAlert --module=app-bootstrap/app-bootstrap.module.ts

// Create AccordionOpenPanelAtATime
ng g c app-bootstrap/components/bootstrap-accordion/accordion-open-panel-at-a-time/AccordionOpenPanelAtATime --module=app-bootstrap/app-bootstrap.module.ts

// Create AccordionToggle
ng g c app-bootstrap/components/bootstrap-accordion/AccordionToggel --module=app-bootstrap/app-bootstrap.module.ts

// Create AccordionHeader
ng g c app-bootstrap/components/bootstrap-accordion/AccordionHeader --module=app-bootstrap/app-bootstrap.module.ts

// Create AccordionPreventChange
ng g c app-bootstrap/components/bootstrap-accordion/AccordionPreventChange --module=app-bootstrap/app-bootstrap.module.ts

// Create AccordionConfig
ng g c app-bootstrap/components/bootstrap-accordion/AccordionConfig --module=app-bootstrap/app-bootstrap.module.ts

// Create AlertBasic
ng g c app-bootstrap/components/bootstrap-alert/AlertBasic --module=app-bootstrap/app-bootstrap.module.ts

// Create ClosableAlert
ng g c app-bootstrap/components/bootstrap-alert/ClosableAlert --module=app-bootstrap/app-bootstrap.module.ts

// Create AlertSelfClosing
ng g c app-bootstrap/components/bootstrap-alert/AlertSelfClosing --module=app-bootstrap/app-bootstrap.module.ts